.text
    li a1, 33
    li a2, 637
    jal ra, mult
    li a7, 1
    ecall
    li a7, 10
    ecall
    

mult:
    li a0, 0
    and t0, a1, a2
    beq t0, zero, _ZeroMult
    blt a2, a1, _Aux01
    mv t0, a1
    mv a1, a2
    j _Ciclo
    _Aux01:
    mv t0, a2    
    _Ciclo:
        add a0, a0, a1
        addi t0, t0, -1
        bne t0, zero, _Ciclo
    ret
    _ZeroMult:
        li a0, 0
        ret
    
